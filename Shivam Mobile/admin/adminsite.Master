                        <asp:MenuItem Text="Contact" Value="Contact" NavigateUrl="~/admin/adm_contact.aspx"></asp:MenuItem>
                        <asp:MenuItem Text="Logout" Value="Logout" NavigateUrl="~/admin/logout.aspx"></asp:MenuItem>
                    </Items>
                      <StaticHoverStyle ForeColor="White" />
