            <%--logo--%>
            <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
              <a class="navbar-brand" href="default.aspx">
                <img src="../Icon/logo.png" alt="Shivam Mobile" height="75px" style="width:200px;" /></a>

              <%--navigation--%>              
                

                <div class="mx-auto" id="navbarNav">
                    <h4>
                    
                    <asp:Menu ID="AdMenu" runat="server" Orientation="Horizontal" style="text-align: left" CssClass="navbar-nav ml-auto" ForeColor="White" StaticSubMenuIndent="25px" OnMenuItemClick="Menu2_MenuItemClick">
                    <DynamicHoverStyle ForeColor="White" />
                    <DynamicMenuItemStyle BackColor="#212529" ForeColor="#CCCCCC" HorizontalPadding="20px" ItemSpacing="5px" VerticalPadding="10px" />
                    <Items>
                        <asp:MenuItem Text="Product"  Value="Product" NavigateUrl="~/admin/viewproduct.aspx">
